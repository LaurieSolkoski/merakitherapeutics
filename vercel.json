{
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      },
      {
        "src": "/style/(.*)",
        "dest": "/public/style/$1"
      },
      {
        "src": "/images/(.*)",
        "dest": "/public/images/lavender_fields.gif"
      }
    ]
  }
  